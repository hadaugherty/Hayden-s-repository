                t2 = "F";
            } else if (key2string.equals("B") && step2string.equals("2.5 steps")) {
                t2 = "F#";
            }


            /** For 3 steps */


            if (key2string.equals("C") && step2string.equals("3 steps")) {
                t2 = "F#";
            } else if (key2string.equals("C#/Db") && step2string.equals("3 steps")) {
                t2 = "G";
            } else if (key2string.equals("D") && step2string.equals("3 steps")) {
                t2 = "Ab";
            } else if (key2string.equals("Eb/D#") && step2string.equals("3 steps")) {
                t2 = "A";
            } else if (key2string.equals("E") && step2string.equals("3 steps")) {
                t2 = "Bb";
            } else if (key2string.equals("F") && step2string.equals("3 steps")) {
                t2 = "B";
            } else if (key2string.equals("F#/Gb") && step2string.equals("3 steps")) {
                t2 = "C";

            } else if (key2string.equals("G") && step2string.equals("3 steps")) {
                t2 = "C#";
            } else if (key2string.equals("Ab/G#") && step2string.equals("3 steps")) {
                t2 = "D";
            } else if (key2string.equals("A") && step2string.equals("3 steps")) {
                t2 = "Eb";
            } else if (key2string.equals("Bb") && step2string.equals("3 steps")) {
                t2 = "E";
            } else if (key2string.equals("B") && step2string.equals("3 steps")) {
                t2 = "F";
            }


            /** For 3.5 steps */


            if (key2string.equals("C") && step2string.equals("3.5 steps")) {
                t2 = "F";
            } else if (key2string.equals("C#/Db") && step2string.equals("3.5 steps")) {
                t2 = "F#";
            } else if (key2string.equals("D") && step2string.equals("3.5 steps")) {
                t2 = "G";
            } else if (key2string.equals("Eb/D#") && step2string.equals("3.5 steps")) {
                t2 = "Ab";
            } else if (key2string.equals("E") && step2string.equals("3.5 steps")) {
                t2 = "A";
            } else if (key2string.equals("F") && step2string.equals("3.5 steps")) {
                t2 = "Bb";
            } else if (key2string.equals("F#/Gb") && step2string.equals("3.5 steps")) {
                t2 = "B";

            } else if (key2string.equals("G") && step2string.equals("3.5 steps")) {
                t2 = "C";
            } else if (key2string.equals("Ab/G#") && step2string.equals("3.5 steps")) {
                t2 = "C#";
            } else if (key2string.equals("A") && step2string.equals("3.5 steps")) {
                t2 = "D";
            } else if (key2string.equals("Bb") && step2string.equals("3.5 steps")) {
                t2 = "Eb";
            } else if (key2string.equals("B") && step2string.equals("3.5 steps")) {
                t2 = "E";
            }


            if (step2string.equals("0 steps")){
                t2=key2string;
            }
            return t2;

        }


/** for 3rd row */

        public String transposed3 (String key3, String step3)
        {


            String key3string = key3;
            String step3string = step3;
            String t3 = "";

            /**For the .5 steps */

            if (key3string.equals("C") && step3string.equals("0 steps")) {
                t3 = "C";
            }
         else   if (key3string.equals("C") && step3string.equals(".5 steps")) {
                t3 = "B";
            } else if (key3string.equals("C#/Db") && step3string.equals(".5 steps")) {
                t3 = "C";
            } else if (key3string.equals("D") && step3string.equals(".5 steps")) {
                t3 = "C#";
            } else if (key3string.equals("Eb/D#") && step3string.equals(".5 steps")) {
                t3 = "D";
            } else if (key3string.equals("E") && step3string.equals(".5 steps")) {
                t3 = "Eb";
            } else if (key3string.equals("F") && step3string.equals(".5 steps")) {
                t3 = "E";
            } else if (key3string.equals("F#/Gb") && step3string.equals(".5 steps")) {
