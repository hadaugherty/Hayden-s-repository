                t5 = "D";

            } else if (key5string.equals("G") && step5string.equals("2 steps")) {
                t5 = "Eb";
            } else if (key5string.equals("Ab/G#") && step5string.equals("2 steps")) {
                t5 = "E";
            } else if (key5string.equals("A") && step5string.equals("2 steps")) {
                t5 = "F";
            } else if (key5string.equals("Bb") && step5string.equals("2 steps")) {
                t5 = "F#";
            } else if (key5string.equals("B") && step5string.equals("2 steps")) {
                t5 = "G";
            }


            /** for 2.5 steps */


            if (key5string.equals("C") && step5string.equals("2.5 steps")) {
                t5 = "G";
            } else if (key5string.equals("C#/Db") && step5string.equals("2.5 steps")) {
                t5 = "Ab";
            } else if (key5string.equals("D") && step5string.equals("2.5 steps")) {
                t5 = "A";
            } else if (key5string.equals("Eb/D#") && step5string.equals("2.5 steps")) {
                t5 = "Bb";
            } else if (key5string.equals("E") && step5string.equals("2.5 steps")) {
                t5 = "B";
            } else if (key5string.equals("F") && step5string.equals("2.5 steps")) {
                t5 = "C";
            } else if (key5string.equals("F#/Gb") && step5string.equals("2.5 steps")) {
                t5 = "C#";

            } else if (key5string.equals("G") && step5string.equals("2.5 steps")) {
                t5 = "D";
            } else if (key5string.equals("Ab/G#") && step5string.equals("2.5 steps")) {
                t5 = "Eb";
            } else if (key5string.equals("A") && step5string.equals("2.5 steps")) {
                t5 = "E";
            } else if (key5string.equals("Bb") && step5string.equals("2.5 steps")) {
                t5 = "F";
            } else if (key5string.equals("B") && step5string.equals("2.5 steps")) {
                t5 = "F#";
            }


            /** For 3 steps */


            if (key5string.equals("C") && step5string.equals("3 steps")) {
                t5 = "F#";
            } else if (key5string.equals("C#/Db") && step5string.equals("3 steps")) {
                t5 = "G";
            } else if (key5string.equals("D") && step5string.equals("3 steps")) {
                t5 = "Ab";
            } else if (key5string.equals("Eb/D#") && step5string.equals("3 steps")) {
                t5 = "A";
            } else if (key5string.equals("E") && step5string.equals("3 steps")) {
                t5 = "Bb";
            } else if (key5string.equals("F") && step5string.equals("3 steps")) {
                t5 = "B";
            } else if (key5string.equals("F#/Gb") && step5string.equals("3 steps")) {
                t5 = "C";

            } else if (key5string.equals("G") && step5string.equals("3 steps")) {
                t5 = "C#";
            } else if (key5string.equals("Ab/G#") && step5string.equals("3 steps")) {
                t5 = "D";
            } else if (key5string.equals("A") && step5string.equals("3 steps")) {
                t5 = "Eb";
            } else if (key5string.equals("Bb") && step5string.equals("3 steps")) {
                t5 = "E";
            } else if (key5string.equals("B") && step5string.equals("3 steps")) {
                t5 = "F";
            }


            /** For 3.5 steps */


            if (key5string.equals("C") && step5string.equals("3.5 steps")) {
                t5 = "F";
            } else if (key5string.equals("C#/Db") && step5string.equals("3.5 steps")) {
                t5 = "F#";
            } else if (key5string.equals("D") && step5string.equals("3.5 steps")) {
                t5 = "G";
            } else if (key5string.equals("Eb/D#") && step5string.equals("3.5 steps")) {
                t5 = "Ab";
            } else if (key5string.equals("E") && step5string.equals("3.5 steps")) {
                t5 = "A";
            } else if (key5string.equals("F") && step5string.equals("3.5 steps")) {
                t5 = "Bb";
            } else if (key5string.equals("F#/Gb") && step5string.equals("3.5 steps")) {
                t5 = "B";

            } else if (key5string.equals("G") && step5string.equals("3.5 steps")) {
                t5 = "C";
            } else if (key5string.equals("Ab/G#") && step5string.equals("3.5 steps")) {
                t5 = "C#";
            } else if (key5string.equals("A") && step5string.equals("3.5 steps")) {
                t5 = "D";
            } else if (key5string.equals("Bb") && step5string.equals("3.5 steps")) {
                t5 = "Eb";
            } else if (key5string.equals("B") && step5string.equals("3.5 steps")) {
                t5 = "E";
            }


            if (step5string.equals("0 steps")){
                t5=key5string;
            }
            return t5;

        }

    public String transposed6 (String key6, String step6)
    {


        String key6string = key6;
        String step6string = step6;
        String t6 = "";

        /**For the .5 steps */

        if (key6string.equals("C") && step6string.equals("0 steps")) {
            t6 = "C";
        }
    else    if (key6string.equals("C") && step6string.equals(".5 steps")) {
            t6 = "B";
        } else if (key6string.equals("C#/Db") && step6string.equals(".5 steps")) {
            t6 = "C";
        } else if (key6string.equals("D") && step6string.equals(".5 steps")) {
            t6 = "C#";
        } else if (key6string.equals("Eb/D#") && step6string.equals(".5 steps")) {
            t6 = "D";
        } else if (key6string.equals("E") && step6string.equals(".5 steps")) {
            t6 = "Eb";
        } else if (key6string.equals("F") && step6string.equals(".5 steps")) {
            t6 = "E";
        } else if (key6string.equals("F#/Gb") && step6string.equals(".5 steps")) {
            t6 = "F";

        } else if (key6string.equals("G") && step6string.equals(".5 steps")) {
            t6 = "F#";
        } else if (key6string.equals("Ab/G#") && step6string.equals(".5 steps")) {
            t6 = "G";
        } else if (key6string.equals("A") && step6string.equals(".5 steps")) {
            t6 = "Ab";
        } else if (key6string.equals("Bb") && step6string.equals(".5 steps")) {
            t6 = "A";
        } else if (key6string.equals("B") && step6string.equals(".5 steps")) {
            t6 = "Bb";
        }

        /**FOR 1 WHOLE STEP */


        if (key6string.equals("C") && step6string.equals("1 step")) {
            t6 = "Bb";
        } else if (key6string.equals("C#/Db") && step6string.equals("1 step")) {
            t6 = "B";
        } else if (key6string.equals("D") && step6string.equals("1 step")) {
            t6 = "C";
        } else if (key6string.equals("Eb/D#") && step6string.equals("1 step")) {
            t6 = "C#";
        } else if (key6string.equals("E") && step6string.equals("1 step")) {
            t6 = "D";
        } else if (key6string.equals("F") && step6string.equals("1 step")) {
            t6 = "Eb";
        } else if (key6string.equals("F#/Gb") && step6string.equals("1 step")) {
            t6 = "E";

        } else if (key6string.equals("G") && step6string.equals("1 step")) {
            t6 = "F";
        } else if (key6string.equals("Ab/G#") && step6string.equals("1 step")) {
            t6 = "F#";
        } else if (key6string.equals("A") && step6string.equals("1 step")) {
            t6 = "G";
        } else if (key6string.equals("Bb") && step6string.equals("1 step")) {
            t6 = "Ab";
        } else if (key6string.equals("B") && step6string.equals("1 step")) {
            t6 = "A";
        }


        /** FOR 1.5 STEPS */

        if (key6string.equals("C") && step6string.equals("1.5 steps")) {
            t6 = "A";
        } else if (key6string.equals("C#/Db") && step6string.equals("1.5 steps")) {
            t6 = "Bb";
        } else if (key6string.equals("D") && step6string.equals("1.5 steps")) {
            t6 = "B";
        } else if (key6string.equals("Eb/D#") && step6string.equals("1.5 steps")) {
            t6 = "C";
        } else if (key6string.equals("E") && step6string.equals("1.5 steps")) {
            t6 = "C#";
        } else if (key6string.equals("F") && step6string.equals("1.5 steps")) {
            t6 = "D";
        } else if (key6string.equals("F#/Gb") && step6string.equals("1.5 steps")) {
            t6 = "Eb";

        } else if (key6string.equals("G") && step6string.equals("1.5 steps")) {
            t6 = "E";
        } else if (key6string.equals("Ab/G#") && step6string.equals("1.5 steps")) {
            t6 = "F";
        } else if (key6string.equals("A") && step6string.equals("1.5 steps")) {
            t6 = "F#";
        } else if (key6string.equals("Bb") && step6string.equals("1.5 steps")) {
            t6 = "G";
        } else if (key6string.equals("B") && step6string.equals("1.5 steps")) {
            t6 = "Ab";
        }


        /** FOR 2 STEPS */


        if (key6string.equals("C") && step6string.equals("2 steps")) {
            t6 = "Ab";
        } else if (key6string.equals("C#/Db") && step6string.equals("2 steps")) {
            t6 = "A";
        } else if (key6string.equals("D") && step6string.equals("2 steps")) {
            t6 = "Bb";
        } else if (key6string.equals("Eb/D#") && step6string.equals("2 steps")) {
            t6 = "B";
        } else if (key6string.equals("E") && step6string.equals("2 steps")) {
            t6 = "C";
        } else if (key6string.equals("F") && step6string.equals("2 steps")) {
            t6 = "C#";
        } else if (key6string.equals("F#/Gb") && step6string.equals("2 steps")) {
            t6 = "D";

        } else if (key6string.equals("G") && step6string.equals("2 steps")) {
            t6 = "Eb";
        } else if (key6string.equals("Ab/G#") && step6string.equals("2 steps")) {
            t6 = "E";
        } else if (key6string.equals("A") && step6string.equals("2 steps")) {
            t6 = "F";
        } else if (key6string.equals("Bb") && step6string.equals("2 steps")) {
            t6 = "F#";
        } else if (key6string.equals("B") && step6string.equals("2 steps")) {
            t6 = "G";
        }


        /** for 2.5 steps */


        if (key6string.equals("C") && step6string.equals("2.5 steps")) {
            t6 = "G";
        } else if (key6string.equals("C#/Db") && step6string.equals("2.5 steps")) {
            t6 = "Ab";
        } else if (key6string.equals("D") && step6string.equals("2.5 steps")) {
            t6 = "A";
        } else if (key6string.equals("Eb/D#") && step6string.equals("2.5 steps")) {
            t6 = "Bb";
        } else if (key6string.equals("E") && step6string.equals("2.5 steps")) {
            t6 = "B";
        } else if (key6string.equals("F") && step6string.equals("2.5 steps")) {
            t6 = "C";
        } else if (key6string.equals("F#/Gb") && step6string.equals("2.5 steps")) {
            t6 = "C#";

        } else if (key6string.equals("G") && step6string.equals("2.5 steps")) {
            t6 = "D";
        } else if (key6string.equals("Ab/G#") && step6string.equals("2.5 steps")) {
            t6 = "Eb";
        } else if (key6string.equals("A") && step6string.equals("2.5 steps")) {
            t6 = "E";
        } else if (key6string.equals("Bb") && step6string.equals("2.5 steps")) {
            t6 = "F";
        } else if (key6string.equals("B") && step6string.equals("2.5 steps")) {
            t6 = "F#";
        }


        /** For 3 steps */


        if (key6string.equals("C") && step6string.equals("3 steps")) {
            t6 = "F#";
        } else if (key6string.equals("C#/Db") && step6string.equals("3 steps")) {
            t6 = "G";
        } else if (key6string.equals("D") && step6string.equals("3 steps")) {
            t6 = "Ab";
        } else if (key6string.equals("Eb/D#") && step6string.equals("3 steps")) {
            t6 = "A";
        } else if (key6string.equals("E") && step6string.equals("3 steps")) {
            t6 = "Bb";
        } else if (key6string.equals("F") && step6string.equals("3 steps")) {
            t6 = "B";
        } else if (key6string.equals("F#/Gb") && step6string.equals("3 steps")) {
            t6 = "C";

        } else if (key6string.equals("G") && step6string.equals("3 steps")) {
            t6 = "C#";
        } else if (key6string.equals("Ab/G#") && step6string.equals("3 steps")) {
            t6 = "D";
        } else if (key6string.equals("A") && step6string.equals("3 steps")) {
            t6 = "Eb";
        } else if (key6string.equals("Bb") && step6string.equals("3 steps")) {
            t6 = "E";
        } else if (key6string.equals("B") && step6string.equals("3 steps")) {
            t6 = "F";
        }


        /** For 3.5 steps */


        if (key6string.equals("C") && step6string.equals("3.5 steps")) {
            t6 = "F";
        } else if (key6string.equals("C#/Db") && step6string.equals("3.5 steps")) {
            t6 = "F#";
        } else if (key6string.equals("D") && step6string.equals("3.5 steps")) {
            t6 = "G";
        } else if (key6string.equals("Eb/D#") && step6string.equals("3.5 steps")) {
            t6 = "Ab";
        } else if (key6string.equals("E") && step6string.equals("3.5 steps")) {
            t6 = "A";
        } else if (key6string.equals("F") && step6string.equals("3.5 steps")) {
            t6 = "Bb";
        } else if (key6string.equals("F#/Gb") && step6string.equals("3.5 steps")) {
            t6 = "B";

        } else if (key6string.equals("G") && step6string.equals("3.5 steps")) {
            t6 = "C";
        } else if (key6string.equals("Ab/G#") && step6string.equals("3.5 steps")) {
            t6 = "C#";
        } else if (key6string.equals("A") && step6string.equals("3.5 steps")) {
            t6 = "D";
        } else if (key6string.equals("Bb") && step6string.equals("3.5 steps")) {
            t6 = "Eb";
        } else if (key6string.equals("B") && step6string.equals("3.5 steps")) {
            t6 = "E";
        }

     if (step6string.equals("0 steps")){
            t6=key6string;
     }
        return t6;


    }


