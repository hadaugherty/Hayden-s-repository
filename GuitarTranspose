/** For 4th row */

        public String transposed4 (String key4, String step4)
        {


            String key4string = key4;
            String step4string = step4;
            String t4 = "";

            /**For the .5 steps */

            if (key4string.equals("C") && step4string.equals("0 steps")) {
                t4 = "C";
            }
        else    if (key4string.equals("C") && step4string.equals(".5 steps")) {
                t4 = "B";
            } else if (key4string.equals("C#/Db") && step4string.equals(".5 steps")) {
                t4 = "C";
            } else if (key4string.equals("D") && step4string.equals(".5 steps")) {
                t4 = "C#";
            } else if (key4string.equals("Eb/D#") && step4string.equals(".5 steps")) {
                t4 = "D";
            } else if (key4string.equals("E") && step4string.equals(".5 steps")) {
                t4 = "Eb";
            } else if (key4string.equals("F") && step4string.equals(".5 steps")) {
                t4 = "E";
            } else if (key4string.equals("F#/Gb") && step4string.equals(".5 steps")) {
                t4 = "F";

            } else if (key4string.equals("G") && step4string.equals(".5 steps")) {
                t4 = "F#";
            } else if (key4string.equals("Ab/G#") && step4string.equals(".5 steps")) {
                t4 = "G";
            } else if (key4string.equals("A") && step4string.equals(".5 steps")) {
                t4 = "Ab";
            } else if (key4string.equals("Bb") && step4string.equals(".5 steps")) {
                t4 = "A";
            } else if (key4string.equals("B") && step4string.equals(".5 steps")) {
                t4 = "Bb";
            }

            /**FOR 1 WHOLE STEP */


            if (key4string.equals("C") && step4string.equals("1 step")) {
                t4 = "Bb";
            } else if (key4string.equals("C#/Db") && step4string.equals("1 step")) {
                t4 = "B";
            } else if (key4string.equals("D") && step4string.equals("1 step")) {
                t4 = "C";
            } else if (key4string.equals("Eb/D#") && step4string.equals("1 step")) {
                t4 = "C#";
            } else if (key4string.equals("E") && step4string.equals("1 step")) {
                t4 = "D";
            } else if (key4string.equals("F") && step4string.equals("1 step")) {
                t4 = "Eb";
            } else if (key4string.equals("F#/Gb") && step4string.equals("1 step")) {
                t4 = "E";

