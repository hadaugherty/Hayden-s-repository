                t4 = "D";
            } else if (key4string.equals("Bb") && step4string.equals("3.5 steps")) {
                t4 = "Eb";
            } else if (key4string.equals("B") && step4string.equals("3.5 steps")) {
                t4 = "E";
            }

            if (step4string.equals("0 steps")){
