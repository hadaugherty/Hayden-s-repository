            } else if (key2string.equals("F") && step2string.equals("2 steps")) {
                t2 = "C#";
            } else if (key2string.equals("F#/Gb") && step2string.equals("2 steps")) {
                t2 = "D";

            } else if (key2string.equals("G") && step2string.equals("2 steps")) {
