                t2 = "F#";
            } else if (key2string.equals("B") && step2string.equals("2 steps")) {
                t2 = "G";
            }


            /** for 2.5 steps */


            if (key2string.equals("C") && step2string.equals("2.5 steps")) {
                t2 = "G";
            } else if (key2string.equals("C#/Db") && step2string.equals("2.5 steps")) {
                t2 = "Ab";
            } else if (key2string.equals("D") && step2string.equals("2.5 steps")) {
                t2 = "A";
            } else if (key2string.equals("Eb/D#") && step2string.equals("2.5 steps")) {
                t2 = "Bb";
            } else if (key2string.equals("E") && step2string.equals("2.5 steps")) {
                t2 = "B";
            } else if (key2string.equals("F") && step2string.equals("2.5 steps")) {
                t2 = "C";
            } else if (key2string.equals("F#/Gb") && step2string.equals("2.5 steps")) {
